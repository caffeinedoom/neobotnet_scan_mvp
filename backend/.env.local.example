# Local Development Environment Configuration
# ===========================================
# Copy this file to .env.local and fill in your actual values

# FastAPI Configuration
PROJECT_NAME=neobotnet-v2
ENVIRONMENT=local-dev
DEBUG=true

# Redis Configuration (Local)
REDIS_HOST=localhost
REDIS_PORT=6379

# Supabase Configuration (Copy from your production .env or Supabase dashboard)
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# JWT Configuration (Copy from production or generate new)
JWT_SECRET_KEY=your_jwt_secret_key_here

# Local Development Settings
LOCAL_TESTING=true
MOCK_ECS_TASKS=true

# AWS Configuration (Optional for local testing - only needed for cloud integration tests)
# AWS_REGION=us-east-1
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key

# CORS Configuration (Optional - defaults are fine for local)
# ALLOWED_ORIGINS=["http://localhost:3000","http://127.0.0.1:3000"]
